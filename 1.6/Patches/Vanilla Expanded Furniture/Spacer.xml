<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded - Spacer Module</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

                <!-- ========== SEATING ALTERNATIVES GROUP ========== -->
                <!-- Advanced Chair (Master) -->
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ThingDef[defName="Seat_AdvChair"]</xpath>
                    <value>
                        <modExtensions>
                            <li Class="SK_Building_Alternatives_Framework.AlternativesModExtension">
                                <tag>VFE_Spacer_SeatingAlternatives</tag>
                                <isMaster>true</isMaster>
                                <uiIconPath>UI/List</uiIconPath>
                                <hoverUiIconPath>UI/ListSelected</hoverUiIconPath>
                            </li>
                        </modExtensions>
                    </value>
                </li>

                <!-- Modern Armchair (Alternative) -->
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ThingDef[defName="VFES_ModernArmchair"]</xpath>
                    <value>
                        <modExtensions>
                            <li Class="SK_Building_Alternatives_Framework.AlternativesModExtension">
                                <tag>VFE_Spacer_SeatingAlternatives</tag>
                                <isMaster>false</isMaster>
                                <hideFromGUI>true</hideFromGUI>
                            </li>
                        </modExtensions>
                    </value>
                </li>

                <!-- Modern Couch (Alternative) -->
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ThingDef[defName="VFES_ModernCouch"]</xpath>
